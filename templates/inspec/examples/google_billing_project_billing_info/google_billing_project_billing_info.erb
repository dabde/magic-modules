<% gcp_project_id = "#{external_attribute('gcp_project_id', doc_generation)}" -%>
describe google_billing_project_billing_info(project_id: <%= doc_generation ? "#{gcp_project_id}" : "gcp_project_id" -%>) do
  it { should exist }

  its('billing_enabled') { should eq true }
end

describe google_billing_project_billing_info(project_id: 'something-else') do
  it { should_not exist }
end