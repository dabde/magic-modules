func resourceKmsCryptoKeyMigrateState(v int, s *terraform.InstanceState, meta interface{}) (*terraform.InstanceState, error) {
	if s.Empty() {
		log.Println("[DEBUG] Empty InstanceState; nothing to migrate.")
		return s, nil
	}

	switch v {
	case 0:
		log.Println("[INFO] Found Google CryptoKey State v0; migrating to v1")
		s, err := migrateKmsCryptoKeyStateV0toV1(s, meta.(*Config))
		if err != nil {
			return s, err
		}
		return s, nil
	default:
		return s, fmt.Errorf("Unexpected schema version: %d", v)
	}
}

func migrateKmsCryptoKeyStateV0toV1(s *terraform.InstanceState, config *Config) (*terraform.InstanceState, error) {
	log.Printf("[DEBUG] Attributes before migration: %#v", s.Attributes)

	keyRingId := s.Attributes["key_ring"]
	parsed, err := parseKmsKeyRingId(keyRingId, config)
	if err != nil {
		return nil, err
	}
	s.Attributes["key_ring"] = parsed.keyRingId()

	log.Printf("[DEBUG] Attributes after migration: %#v", s.Attributes)
	return s, nil
}
