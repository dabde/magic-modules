func expand<%= prefix -%><%= titlelize_property(property) -%>(v interface{}, d *schema.ResourceData, config *Config) (interface{}, error) {
	if v == nil || v.(string) == "" {
		return "", nil
	}
    router := v.(string)
	if !strings.HasPrefix(router, "https://www.googleapis.com/compute/") {
		// Router value provided is just the name, lookup the router SelfLink
        project, err := getProject(d, config)
        if err != nil {
            return nil, err
        }
        region, err := getRegion(d, config)
        if err != nil {
            return nil, err
        }
		routerData, err := config.clientCompute.Routers.Get(
			project, region, router).Do()
		if err != nil {
			return nil, fmt.Errorf("Error reading router: %s", err)
		}
		router = routerData.SelfLink
	}

	return router, nil

}
