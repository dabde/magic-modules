// If we need to delete contents, we should customize the delete URL
if d.Get("delete_contents_on_destroy").(bool) {
	log.Printf("[DEBUG] replacing URL %q with a custom delete URL", url)
	url, err = replaceVars(d, config, "<%= "{{#{object.__product.name}BasePath}}" -%><%=object.self_link-%>?deleteContents=true")
	if err != nil {
		return err
	}	
}
