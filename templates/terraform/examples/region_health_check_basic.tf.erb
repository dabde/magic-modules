provider "google-beta" {
  region = "us-central1"
  zone = "us-central1-a"
}

resource "google_compute_region_health_check" "internal-region-health-check" {
 provider = "google-beta"

 name = "<%= ctx[:vars]['region_health_check_name'] %>"
 region = "us-central1"

 timeout_sec        = 1
 check_interval_sec = 1

 tcp_health_check {
   port = "80"
 }
}

